{
  "videoSupport": {
    "comment": "Control buttons, video name labels, etc.",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=6361%3A14",
    "controls": {
      "button": {
        "#normal": {
          "background": "@theme-button-inverted-normal",
          "text": "@theme-text-primary-normal",
          "icon": "@theme-text-primary-normal",
          "border": "@theme-outline-button-normal"
        },
        "#hovered": {
          "background": "@theme-button-inverted-hover",
          "text": "@theme-text-primary-normal",
          "icon": "@theme-text-primary-normal",
          "border": "@theme-outline-button-normal"
        },
        "#pressed": {
          "background": "@theme-button-inverted-pressed",
          "text": "@theme-text-primary-normal",
          "icon": "@theme-text-primary-normal",
          "border": "@theme-outline-button-normal"
        },
        "#disabled": {
          "background": "@theme-button-inverted-normal",
          "text": "@theme-text-primary-disabled",
          "icon": "@theme-text-primary-disabled",
          "border": "@theme-outline-disabled-normal"
        }
      }
    },
    "videoNameLabels": {
      "videoOff": {
        "background": "@theme-background-primary-ghost",
        "name": "@theme-text-primary-normal",
        "icon": "@theme-text-primary-normal",
        "role": "@theme-text-secondary-normal",
        "iconMute": "@theme-text-error-normal",
        "iconUnmute": "@theme-text-success-normal"
      },
      "videoOn": {
        "background": "@theme-background-namelabel-normal",
        "name": "@theme-common-text-white",
        "icon": "@theme-common-text-white",
        "role": "@theme-text-secondary-normal",
        "iconMute": "@theme-common-text-error",
        "iconUnmute": "@theme-common-text-success"
      }
    },
    "slider": {
      "track": {
          "fill": "@theme-control-active-normal",
          "empty": "@theme-outline-button-normal"
      },
      "mover": {
          "border": "@theme-outline-primary-normal",
          "background": "@theme-background-solid-tertiary-normal"
      },
      "border": "@theme-outline-button-normal",
      "background": "@theme-button-inverted-normal"
    }
  }
}
